body
	background: #F2F2F2


header
	position: relative	
	.logo 
		background: #FF9933
		box-shadow: 10px 10px 20px rgba(black, 0.5)
		text-align: center
		margin: 100px

	figure
		img
			border-radius: 50%
			height: 90px
			//left: 600px
			margin: 5px
			padding: 0
			position: absolute
			top -100px
			width: 90px

.panel-principal
	background: #C0C0C0
	box-shadow: 4px 4px 4px 4px rgba(black, 0.5)
	.galeria
		figure
			img
				background: #C0C0C0
				box-shadow 4px 4px 6px rgba(orange, 0.5)
				border-radius: 1em
				height: 110px
				margin: 30px
				width: 130px

